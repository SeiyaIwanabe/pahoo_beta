<div class="createBannerArea">
  <%= form_with(model: @community,  local: true) do |f| %>

    <div class="bannerSection">
      <%= f.label :community_icon, "コミュニティのイメージ画像を選択" %>
      <%= f.file_field :community_icon %>
    </div>

    <div class="bannerSection">
      <%= f.label :community_title, "コミュニティのタイトル"%>
      <%= f.text_field :community_title, placeholder: "コミュニティのタイトルを入力"%>
    </div>

    <%= f.submit "コミュニティを作成", class: "submitBtn" %>

  <% end %>
  <%= render partial: 'modules/footer' %>
</div>